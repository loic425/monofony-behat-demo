{
    "name": "monofony/skeleton",
    "type": "project",
    "description": "A Minimal Monofony project to create applications.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "api-platform/core": "^3.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/migrations": "^3.2",
        "doctrine/orm": "^2.5",
        "fakerphp/faker": "^1.14",
        "liip/imagine-bundle": "^2.2",
        "monofony/admin": "^0.9@dev",
        "monofony/admin-contracts": "^0.9@dev",
        "monofony/admin-meta": "^0.9@dev",
        "monofony/core-bundle": "^0.9@dev",
        "monofony/core-contracts": "^0.9@dev",
        "monofony/core-meta": "^0.9@dev",
        "monofony/sylius-user-bridge": "^0.9@dev",
        "nelmio/cors-bundle": "^2.3",
        "pagerfanta/pagerfanta": "^3.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "sonata-project/block-bundle": "^4.2",
        "sylius/customer-bundle": "^1.12",
        "sylius/grid-bundle": "^1.12",
        "sylius/mailer": "^2.0",
        "sylius/resource-bundle": "^1.10",
        "sylius/ui-bundle": "^1.12",
        "sylius/user-bundle": "^1.12 || ^1.12@dev",
        "symfony/asset": "^5.4 || ^6.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dotenv": "^5.4 || ^6.0",
        "symfony/expression-language": "6.1.*",
        "symfony/flex": "^2.0",
        "symfony/mailer": "^5.4 || ^6.0",
        "symfony/monolog-bundle": "^3.6",
        "symfony/process": "^5.4 || ^6.0",
        "symfony/property-access": "6.1.*",
        "symfony/property-info": "6.1.*",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/runtime": "^5.4 || ^6.0",
        "symfony/security-bundle": "6.1.*",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/templating": "^5.4 || ^6.0",
        "symfony/translation": "^5.4 || ^6.0",
        "symfony/twig-bundle": "6.1.*",
        "symfony/validator": "^5.4 || ^6.0",
        "symfony/webpack-encore-bundle": "^1.9",
        "twig/twig": "^2.7",
        "vich/uploader-bundle": "^1.8",
        "webmozart/assert": "^1.9",
        "zenstruck/foundry": "^1.14"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "behat/mink-selenium2-driver": "^1.3",
        "dbrekelmans/bdi": "^1.0",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "justinrainbow/json-schema": "^5.2",
        "lchrusciel/api-test-case": "^5.1",
        "monofony/behat-bridge": "^0.9@dev",
        "phpspec/phpspec": "^7.2",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-nette": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpstan/phpstan-webmozart-assert": "^1.1",
        "psr/event-dispatcher": "^1.0",
        "robertfausk/behat-panther-extension": "^1.1",
        "se/selenium-server-standalone": "2.*",
        "symfony/debug-bundle": "^5.4 || ^6.0",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^5.4 || ^6.0",
        "symfony/web-profiler-bundle": "^5.4 || ^6.0",
        "vimeo/psalm": "^5.7"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.1.*",
            "endpoint": "flex://defaults"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
